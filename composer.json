{
    "name": "queuewatch/laravel",
    "description": "Laravel package for Queuewatch - Real-time queue failure monitoring",
    "keywords": [
        "queuewatch",
        "laravel",
        "queue",
        "failed-jobs",
        "monitoring",
        "notifications"
    ],
    "homepage": "https://github.com/queuewatch/laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Marian Pop",
            "email": "mvpopuk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.9|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.20|^3.0|^4.0",
        "pestphp/pest-plugin-arch": "^2.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Queuewatch\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Queuewatch\\Laravel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Queuewatch\\Laravel\\QueuewatchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
